<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Regression suite" verbose="1" >
    <groups>
        <run>
            <include name="regression"></include>
        </run>
    </groups>
    <listeners>
        <listener class-name="com.turorialsninja.demo.customlisteners.CustomListeners"/>
        <listener class-name="org.uncommons.reportng.HTMLReporter"/>
        <listener class-name="org.uncommons.reportng.JUnitXMLReporter"/>
    </listeners>
    <test name="Regression Test" >
        <classes>
            <class name="com.turorialsninja.demo.testsuit.AccountLoginPageTest"/>
            <class name="com.turorialsninja.demo.testsuit.DesktopPageTest"/>
            <class name="com.turorialsninja.demo.testsuit.LaptopsAndNotebooksPageTest"/>
            <class name="com.turorialsninja.demo.testsuit.RegisterPageTest"/>
            <class name="com.turorialsninja.demo.testsuit.TopMenuTest"/>
        </classes>
    </test>
</suite>